gradef(y(x), yp(x));
gradef(yp(x), ypp(x));
gradef(ypp(x), yppp(x));
gradef(yppp(x), ypppp(x));

L: ypp(x)^2 / (1+yp(x)^2)^3;

p:
diff(L, y(x))
-
diff(diff(L, yp(x)), x)
+
diff(diff(diff(L, ypp(x)), x), x);

p: num(ratsimp(p));

q: coeff(p, ypppp(x), 1);
r: p - q * ypppp(x);

f: ratsimp(r / q);

