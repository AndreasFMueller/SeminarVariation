f: 2 * x / (1 + yp^2);
fyp: diff(f,yp);

gradef(y(x),yp(x));
gradef(yp(x),ypp(x));

fypx: subst([yp=yp(x),y=y(x)], fyp);

dxfypx: diff(fypx, x);
ratsimp(dxfypx);

H: c * (1+p^2)^2/p;

f: p * diff(H,p) / (p * diff(H,p)-1);
integrate(f,p);
